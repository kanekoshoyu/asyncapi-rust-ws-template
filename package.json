{
  "name": "asyncapi-rust-ws-template",
  "version": "0.2.0",
  "description": "Rust WebSocket client template for AsyncAPI Generator",
  "author": "Sho Kaneko <kanekoshoyu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanekoshoyu/asyncapi-rust-websocket-template.git"
  },
  "keywords": [
    "asyncapi",
    "generator",
    "rust",
    "websocket",
    "tokio",
    "template"
  ],
  "main": "index.js",
  "scripts": {
    "prepare": "tsc",
    "release": "semantic-release",
    "test_v2": "npm run clear_template && npm run clear_output_v2 && npm run prepare && npm run render_v2 && npm run cargo_check_v2",
    "test_v3": "npm run clear_template && npm run clear_output_v3 && npm run prepare && npm run render_v3 && npm run cargo_check_v3",
    "validate_v2": "ag test/input/v2.yaml ./ -o test/output/v2 -p validate=true",
    "validate_v3": "ag test/input/v3.yaml ./ -o test/output/v3 -p validate=true",
    "render_v2": "ag test/input/v2.yaml ./ -o test/output/v2 -p render=true --debug",
    "render_v3": "ag test/input/v3.yaml ./ -o test/output/v3 -p render=true --debug",
    "validate_render_v2": "ag test/input/v2.yaml ./ -o test/output/v2 -p validate=true -p render=true",
    "validate_render_v3": "ag test/input/v3.yaml ./ -o test/output/v3 -p validate=true -p render=true",
    "clear_template": "rm -rf template",
    "clear_output": "rm -rf test/output",
    "clear_output_v2": "rm -rf test/output/v2",
    "clear_output_v3": "rm -rf test/output/v3",
    "cargo_check_v2": "cd test/output/v2 && cargo check && cargo fmt",
    "cargo_check_v3": "cd test/output/v3 && cargo check && cargo fmt",
    "lint": "echo \"No linter specified yet\"",
    "bump:version": "npm --no-git-tag-version --allow-same-version version $VERSION"
  },
  "files": [
    "template"
  ],
  "dependencies": {
    "@asyncapi/generator-filters": "2.1.0",
    "@asyncapi/generator-hooks": "0.1.0",
    "@asyncapi/generator-react-sdk": "1.1.2",
    "@asyncapi/modelina": "3.7.0",
    "node-fetch": "3.3.2",
    "react": "^18.3.1"
  },
  "generator": {
    "renderer": "react",
    "apiVersion": "v3",
    "generator": ">=1.3.0 <3.0.0",
    "parameters": {
      "validate": {
        "description": "validate a file: [true: default, false]",
        "default": true
      },
      "render": {
        "description": "render websocket client: [true: default, false]",
        "default": true
      },
      "framework": {
        "description": "type of websocket framework to render: [tokio-tungstenite, async-tungstenite]",
        "default": "tokio-tungstenite"
      }
    },
    "nonRenderableFiles": [],
    "filters": [],
    "hooks": {}
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  }
}