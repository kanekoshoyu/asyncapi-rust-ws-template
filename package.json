{
  "name": "asyncapi-rust-ws-template",
  "version": "0.1.1",
  "description": "Rust WebSocket client template for AsyncAPI Generator",
  "author": "Sho Kaneko <kanekoshoyu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanekoshoyu/asyncapi-rust-websocket-template.git"
  },
  "keywords": [
    "asyncapi",
    "generator",
    "rust",
    "websocket",
    "tokio",
    "template"
  ],
  "main": "index.js",
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "release": "semantic-release",
    "test": "npm run clear_all && npm run build && npm run validate",
    "validate": "ag sample/b.yaml ./ -o output -p validate=true",
    "render": "ag sample/b.yaml ./ -o output -p render=true",
    "validate_render": "ag sample/b.yaml ./ -o output -p validate=true",
    "clear_template": "rm -rf template",
    "clear_output": "rm -rf output",
    "clear_all": "npm run clear_template && npm run clear_output",
    "lint": "echo \"No linter specified yet\"",
    "bump:version": "npm --no-git-tag-version --allow-same-version version $VERSION"
  },
  "dependencies": {
    "@asyncapi/generator-filters": "2.1.0",
    "@asyncapi/generator-hooks": "0.1.0",
    "@asyncapi/generator-react-sdk": "1.1.2",
    "@asyncapi/modelina": "3.7.0",
    "react": "^18.3.1",
    "node-fetch": "3.3.2"
  },
  "generator": {
    "renderer": "react",
    "generator": ">=1.3.0 <3.0.0",
    "parameters": {
      "validate": {
        "description": "validate a file: [true: default, false]",
        "default": false
      },
      "render": {
        "description": "render websocket client: [true: default, false]",
        "default": false
      },
      "framework": {
        "description": "type of websocket framework to render: [tokio-tungstenite, async-tungstenite]",
        "default": "tokio-tungstenite"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  }
}